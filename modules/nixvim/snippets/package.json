{
    "name": "elrohirgt-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "./global.json"
            },
						{
							"language": [
								"go"
							],
							"path": "./go.json"
						},
						{
							"language": "dockerfile",
							"path": "./docker_file.json"
						},
						{
							"language": "cdoc",
							"path": "./c_doc.json"
						},
						{
							"language": "c",
							"path": "./c.json"
						},
						{
							"language": "nix",
							"path": "./nix.json"
						},
						{
							"language": ["markdown", "rmd"],
							"path": "./markdown.json"
						},
						{
							"language": "gleam",
							"path":"./gleam.json"
						}
				]
		}
}
